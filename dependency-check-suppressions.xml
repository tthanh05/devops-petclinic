<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

  <!-- Example suppression for WebJar metadata noise from RetireJS (only if you verified itâ€™s harmless) -->
  <suppress until="2026-01-01"
            justification="WebJar metadata only; no runtime JS executed in this app">
    <!-- Use PURL, not <gav> -->
    <packageUrl regex="true">^pkg:maven/org.webjars/(bootstrap|font-awesome)@.*$</packageUrl>
    <!-- keep it safe: only suppress low/medium scores -->
    <cvssBelow>7.0</cvssBelow>
  </suppress>

</suppressions>
